{
    "name": "SafeCodeInspector",
    "displayName": "Safe Code Inspector",
    "description": "Un'estensione per rilevare deprecazioni e vulnerabilit√† nel codice.",
    "version": "0.1.0",
    "publisher": "Tiziana Maggio",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.safeCodeInspect"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.safeCodeInspect",
